{
  "name": "angular2-seed",
  "version": "1.0.0",
  "scripts": {
    "lite": "lite-server",
    "postinstall": "typings install",
    "start": "npm run build:vendor && npm run build:dev && concurrently \"npm run build:dev.watch\" \"npm run lite\" ",
    "build:vendor": "gulp vendor",
    "build:production": "gulp clean && gulp build && gulp compress",
    "build:production.bundle": "gulp build --production && node systemjs.builder.js && gulp compress",
    "build:dev": "gulp clean && gulp build",
    "build:dev.watch": "gulp clean && gulp",
    "typings": "typings"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.19",
    "concurrently": "^2.0.0",
    "connect-gzip-static": "^1.0.0",
    "del": "^2.2.0",
    "google-closure-compiler": "^20160315.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.0.6",
    "gulp-flatten": "^0.2.0",
    "gulp-gzip": "^1.2.0",
    "gulp-if": "^2.0.1",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.10.0",
    "lite-server": "^2.1.0",
    "systemjs-builder": "^0.15.16",
    "tslint": "^3.9.0",
    "typings": "^0.8.1",
    "yargs": "^4.7.0"
  }
}
